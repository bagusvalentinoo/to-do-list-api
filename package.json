{
  "name": "library-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "start": "node index",
    "dev": "nodemon",
    "make:migration": "sequelize-cli migration:generate --name",
    "make:seeder": "sequelize-cli seed:generate --name",
    "migrate": "sequelize-cli db:migrate",
    "migrate:rollback": "sequelize-cli db:migrate:undo:all",
    "migrate:fresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "migrate:fresh:seed": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "_moduleAliases": {
    "@root": ".",
    "@src": "src",
    "@config": "src/config",
    "@controllers": "src/controllers/api",
    "@database": "src/database",
    "@seeders": "src/database/seeders",
    "@migrations": "src/database/migrations",
    "@helpers": "src/helpers",
    "@middlewares": "src/middlewares",
    "@models": "src/models",
    "@resources": "src/resources",
    "@routes": "src/routes/api",
    "@rules": "src/rules",
    "@services": "src/services",
    "@tasks": "src/tasks"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-firebase-storage": "^4.1.2",
    "mysql2": "^3.8.0",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.1",
    "uuid": "^9.0.1"
  }
}
